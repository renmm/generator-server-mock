{
  "name": "generator-server-mock",
  "version": "0.1.0",
  "description": "一个mock server 生成器，基于koa",
  "homepage": "",
  "author": {
    "name": "renmaomin",
    "email": "renmaomin@126.com",
    "url": "www.renmaomin.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "scripts": {
    "lint": "standard --verbose | snazzy --format",
    "commit:init": "commitizen init cz-conventional-changelog --save --save-exact --force",
    "validate-commit-msg": "npm run validate-commit-msg",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "standard": {
    "ignore": [
      "dist/",
      "**/lib/"
    ],
    "env": {
      "mocha": true
    }
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "mock",
    "server",
    "mock server",
    "yeoman-generator"
  ],
  "dependencies": {
    "yeoman-generator": "^0.23.0",
    "chalk": "^1.0.0",
    "yosay": "^1.0.0"
  },
  "devDependencies": {
    "changelog": "^1.0.7",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.1.1",
    "eslint-config-xo-space": "^0.15.0",
    "ghooks": "^1.3.2",
    "gulp": "^3.9.0",
    "gulp-coveralls": "^0.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-istanbul": "^1.0.0",
    "gulp-line-ending-corrector": "^1.0.1",
    "gulp-mocha": "^3.0.1",
    "gulp-nsp": "^2.1.0",
    "gulp-plumber": "^1.0.0",
    "lodash": "^4.16.4",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2",
    "standard-format": "^2.2.2",
    "validate-commit-msg": "^2.8.2",
    "yeoman-assert": "^2.0.0",
    "yeoman-test": "^1.0.0"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    }
  },
  "repository": "renmm/generator-server-mock",
  "license": "MIT"
}
