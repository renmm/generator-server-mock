    "gulp-sftp": "^0.1.4",
    "gulp-util": "^3.0.6",
    "hammerjs": "^2.0.4",
    "history": "^1.17.0",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.6.0",
    "image-webpack-loader": "^1.6.1",
    "imagemin": "^3.2.0",
    "invariant": "^2.2.0",
    "ip": "^1.1.0",
