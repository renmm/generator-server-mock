{
  "name": "invoice",
  "version": "0.1.0",
  "description": "基于webpack、react搭建发票app",
  "main": "app/main.js",
  "scripts": {
    "start": "nodemon app.js",
    "test": "gulp test",
    "build": "npm install && NODE_ENV=production gulp",
    "ontest": "gulp ontest",
    "online": "gulp online",
    "prebuild": "npm run lint",
    "lint": "standard --verbose | snazzy --format",
    "commit:init": "commitizen init cz-conventional-changelog --save --save-exact",
    "validate-commit-msg": "npm run validate-commit-msg",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "NODE_ENV=production nodemon app.js"
  },
  "standard": {
    "ignore": [
      "dist/",
      "mock/",
      "**/lib/",
      "tmp.js"
    ],
    "env": {
      "mocha": true
    }
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "webpack",
    "react",
    "starter",
    "boilerplate",
    "hot"
  ],
  "author": "renmaomin",
  "license": "MIT",
  "dependencies": {
    "async": "^1.2.0",
    "babel-core": "^5.0.12",
    "babel-loader": "^5.0.0",
